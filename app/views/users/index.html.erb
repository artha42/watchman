<div class="main_section">
  <ul class="users">
  <% @users.each do |user| %>
    <li class="user" id="user-<%=user.id%>">
    <%=link_to user.username, user_path(user.id) %>
    </li>
  <% end %>
  </ul>
</div>
<div class="actions">
  <div>
  <span class="user_add"></span>
  <%=link_to 'New User', new_user_path, :class=>"new_user"%>
  </div>
  <br />
  <%=link_to 'Groups', groups_path %>

</div>