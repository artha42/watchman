<div id="login_form">
<%= error_messages_for :user_session %>
<% form_for @user_session do |f| %>
  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
    </p>
  <%= f.hidden_field :redirect_path %>
  <p><%= f.submit "Submit" %></p>
<% end %>
</div>
